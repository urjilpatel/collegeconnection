    <% for(var i=0;i<books_inquiry_array.length;i++) {%>
    <tbody>
            
        <tr role="row" class="odd">
           
            <td class="sorting_1"><%= i+1%></td>
            <td></td>
            <td></td>
            <td><%= books_inquiry_array[i].inquiry%></td>
            <td><%= books_inquiry_array[i].price%></td>
            <td><%= books_inquiry_array[i].details%></td>
           
        
            
        </tr>
        <% } %>  
        
    </tbody>
</table>

<% } else {%>
    Not Record Found
    <% } %>
</div></div><div class="row">
</div>
</div>
